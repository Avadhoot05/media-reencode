{
    "builds": [
        {
            "src": "./server/app.js", 
            "use": "@vercel/node"
        },

        {
            "src": "./server/client/build",   
            "use": "@vercel/static"
        }
    ], 
    

    "routes": [
        {
          "src": "/server/.*",
          "dest": "server/app.js"
        }
      ]
}